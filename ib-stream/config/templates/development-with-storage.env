# Development Environment with Storage (No Background Recording)
# For testing API with storage retrieval but no background streaming

# Server Configuration
PORT=8101
HOST=0.0.0.0

# TWS Connection (different client ID than production)
IB_STREAM_CLIENT_ID=12
IB_STREAM_HOST=127.0.0.1
IB_STREAM_PORTS=7497,7496,4002,4001

# Storage Configuration (Enable for buffer retrieval)
IB_STREAM_STORAGE_PATH=storage
IB_STREAM_ENABLE_STORAGE=true
IB_STREAM_ENABLE_JSON=true
IB_STREAM_ENABLE_PROTOBUF=true
IB_STREAM_ENABLE_POSTGRES=false

# Tracked Contracts (None for development - no background streaming)
IB_STREAM_TRACKED_CONTRACTS=

# Performance Settings
IB_STREAM_MAX_STREAMS=25
IB_STREAM_BUFFER_SIZE=100
IB_STREAM_CONNECTION_TIMEOUT=5
IB_STREAM_RECONNECT_ATTEMPTS=3

# Logging
IB_STREAM_LOG_LEVEL=DEBUG