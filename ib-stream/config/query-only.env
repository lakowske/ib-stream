# Query-Only Mode - No streaming to storage at startup
# Reads from existing storage dataset without writing new data

# Server Configuration
PORT=8100
HOST=0.0.0.0

# TWS Connection (different client ID to avoid conflicts with production)
IB_STREAM_CLIENT_ID=15
IB_STREAM_HOST=127.0.0.1
IB_STREAM_PORTS=7497,7496,4002,4001

# Storage Configuration - Enable reading from existing dataset
IB_STREAM_STORAGE_PATH=../../ib-stream/ib-stream/storage
IB_STREAM_ENABLE_STORAGE=true
IB_STREAM_ENABLE_JSON=true
IB_STREAM_ENABLE_PROTOBUF=true
IB_STREAM_ENABLE_POSTGRES=false

# Disable client stream storage (no new data written during API calls)
IB_STREAM_ENABLE_CLIENT_STREAM_STORAGE=false

# NO tracked contracts (prevents background streaming at startup)
IB_STREAM_TRACKED_CONTRACTS=

# Performance Settings
IB_STREAM_MAX_STREAMS=25
IB_STREAM_BUFFER_SIZE=100
IB_STREAM_CONNECTION_TIMEOUT=5
IB_STREAM_RECONNECT_ATTEMPTS=3

# Logging
IB_STREAM_LOG_LEVEL=INFO